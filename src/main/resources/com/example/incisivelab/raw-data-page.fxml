<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="700.0" prefWidth="1178.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.incisivelab.RawDataPageController">
   <children>
      <Pane layoutX="183.0" layoutY="81.0" prefHeight="538.0" prefWidth="812.0" style="-fx-border-width: 1px; -fx-border-color: #a3a3a3; -fx-border-radius: 11;">
         <children>
            <TableView fx:id="rawDataTable" layoutX="16.0" layoutY="90.0" prefHeight="302.0" prefWidth="781.0">
               <columns>
                  <TableColumn prefWidth="62.0" text="Sample">
                     <cellValueFactory>
                        <PropertyValueFactory property="sampleName" />
                     </cellValueFactory>
                  </TableColumn>
                  <TableColumn prefWidth="180.0" text="Monomer 14 kDa">
                     <cellValueFactory>
                        <PropertyValueFactory property="monomer" />
                     </cellValueFactory>
                  </TableColumn>
                  <TableColumn prefWidth="180.0" text="Dimer 28 kDa">
                     <cellValueFactory>
                        <PropertyValueFactory property="dimer" />
                     </cellValueFactory>
                  </TableColumn>
                  <TableColumn prefWidth="180.0" text="Trimer 42 kDa">
                     <cellValueFactory>
                        <PropertyValueFactory property="trimer" />
                     </cellValueFactory>
                  </TableColumn>
                  <TableColumn prefWidth="180.0" text="Tretramer 56 kDa">
                     <cellValueFactory>
                        <PropertyValueFactory property="tretramer" />
                     </cellValueFactory>
                  </TableColumn>
               </columns>
            </TableView>
            <Button fx:id="addRowButton" layoutX="724.0" layoutY="52.0" onAction="#addRow" prefHeight="24.0" prefWidth="70.0" style="-fx-background-color: #7ae246;" text="Add" textFill="WHITE" />
            <Button fx:id="removeRowButton" layoutX="693.0" layoutY="405.0" onAction="#removeRow" style="-fx-background-color: #ff564a;" text="Remove Row" textFill="WHITE" />
            <TextField fx:id="sampleNameTxt" layoutX="18.0" layoutY="53.0" prefHeight="24.0" prefWidth="142.0" />
            <TextField fx:id="monomerText" layoutX="159.0" layoutY="53.0" prefHeight="24.0" prefWidth="142.0" />
            <TextField fx:id="dimerText" layoutX="300.0" layoutY="53.0" prefHeight="24.0" prefWidth="142.0" />
            <Label layoutX="18.0" layoutY="29.0" text="Sample Name" />
            <Label layoutX="160.0" layoutY="29.0" text="Monomer 14 kDa" />
            <Label layoutX="302.0" layoutY="29.0" text="Dimer 28 kDa" />
            <Button fx:id="resetBtn" layoutX="369.0" layoutY="465.0" mnemonicParsing="false" onAction="#onResetButtonClick" prefHeight="32.0" prefWidth="77.0" style="-fx-background-color: #0091FF;" text="RESET" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="nextBtn" layoutX="507.0" layoutY="465.0" mnemonicParsing="false" onAction="#onNextButtonClick" prefHeight="32.0" prefWidth="77.0" style="-fx-background-color: #0091FF;" text="NEXT" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="backBtn" layoutX="231.0" layoutY="465.0" mnemonicParsing="false" onAction="#onBackButtonClick" prefHeight="32.0" prefWidth="77.0" style="-fx-background-color: #0091FF;" text="BACK" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <TextField fx:id="trimerText" layoutX="441.0" layoutY="53.0" prefHeight="24.0" prefWidth="142.0" />
            <Label layoutX="444.0" layoutY="29.0" text="Trimer 42 kDa" />
            <TextField fx:id="tretramerText" layoutX="582.0" layoutY="53.0" prefHeight="24.0" prefWidth="142.0" />
            <Label layoutX="584.0" layoutY="29.0" text="Tretramer 56 kDa" />
         </children>
      </Pane>
   </children>
</AnchorPane>
